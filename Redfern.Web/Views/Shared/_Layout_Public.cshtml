@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section Styles{
    @RenderSection("Styles", required: false)
}

@section Scripts{
    @RenderSection("Scripts", false)
    
}

<div id="wrapper">
    <header class="page">
        @Html.Partial("_PubNavigationBar")
